<!-- Aiguilles -->

<PropertyList>
     <condition>
        <and>
            <not>
                 <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </and>
      </condition>
		<type>material</type>
		<emission>
			<factor-prop>fdm/jsbsim/electrical/lighting/instruments-norm</factor-prop>
			<red>0.80</red>
			<green>0.40</green>
			<blue>0.30</blue>
		</emission>
		<diffuse>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</diffuse>
		<ambient>
			<red>01</red>
			<green>1</green>
			<blue>1</blue>
		</ambient>
		<specular>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</specular>
		<shininess> 20</shininess>
</PropertyList>