<PropertyList>
<sim>
    <description>Catalina PBY</description>
    <author> was Gerard Robin , Ron Jensen (engine and propeller) update grtux hangar team https://sites.google.com/site/grtuxhangar/ </author>
    <status>alpha, "GPL V2 Copyright"</status>
     <version>0.6.9_2011-06-026</version>

        <model>
            <path>Aircraft/PBY-Catalina/Models/Catalina_pelican-J.xml</path>
	    <waterloading>0</waterloading>
	    <bomb-signal>0</bomb-signal>
            <Aircraft>
                <navaid><input>
                        <chan-XX type="double">0</chan-XX>
                        <selector>0</selector>
                    </input></navaid>
                <lighting>
                    <instrument-lights type="double">0.2</instrument-lights>
                </lighting>
            </Aircraft>
        </model>

        <submodels>
            <serviceable type="bool">true</serviceable>
            <path>Aircraft/PBY-Catalina/Models/submodels-osg.xml</path>
        </submodels>

        <flight-model>jsb</flight-model>
        <aero>PBY-6-devel</aero>

        <virtual-cockpit archive="y">true</virtual-cockpit>
        <allow-toggle-cockpit archive="y">true</allow-toggle-cockpit>

        <startup>
            <splash-texture>Aircraft/PBY-Catalina/pby6.png</splash-texture>
        </startup>

        <sound>
            <path>Aircraft/PBY-Catalina/Sounds/sound.xml</path>
        </sound>

        <systems>
            <autopilot>
                <path>Aircraft/PBY-Catalina/Systems/PBYautopilot.xml</path>
            </autopilot>
            <electrical>
                <path>Aircraft/PBY-Catalina/Systems/dummy-electrical.xml</path>
            </electrical>
        </systems>

        <instrumentation>
            <path>Aircraft/PBY-Catalina/Systems/instrumentation.xml</path>
        </instrumentation>


        <help include="Help/Catalina-help.xml"/>

        <view n="0">
            <internal archive="y">true</internal>
            <config>
                <!-- x/y/z == right/up/back -->
                <x-offset-m archive="y">-0.48</x-offset-m>
                <y-offset-m archive="y">0.51</y-offset-m>
                <z-offset-m archive="y">-3.78</z-offset-m>
                <pitch-offset-deg archive="y">-18.0</pitch-offset-deg>
                <default-field-of-view-deg type="double">88.0</default-field-of-view-deg>
            </config>
        </view>

        <current-view>
            <field-of-view type="double">65.0</field-of-view>
        </current-view>

        <chase-distance-m>-40</chase-distance-m>

        <view n="100">
            <name>Cockpit View</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <ground-level-nearplane-m type="double">0.05f</ground-level-nearplane-m>
                <default-field-of-view-deg type="double">75.0</default-field-of-view-deg>
                <default-pitch-deg type="double">0</default-pitch-deg>
                <default-heading-deg type="double">0</default-heading-deg>
                <front-direction-deg type="double">0</front-direction-deg>
                <front-left-direction-deg type="double">45</front-left-direction-deg>
                <left-direction-deg type="double">90</left-direction-deg>
                <back-left-direction-deg type="double">135</back-left-direction-deg>
                <back-direction-deg type="double">180</back-direction-deg>
                <back-right-direction-deg type="double">225</back-right-direction-deg>
                <right-direction-deg type="double">270</right-direction-deg>
                <front-right-direction-deg type="double">315</front-right-direction-deg>
                <x-offset-m archive="y">0.0</x-offset-m>
                <y-offset-m archive="y">1.100</y-offset-m>
                <z-offset-m archive="y">0.036</z-offset-m>
                <pitch-offset-deg>0</pitch-offset-deg>
            </config>
        </view>

        <view n="101">
            <name>Cockpit View</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <ground-level-nearplane-m type="double">0.05f</ground-level-nearplane-m>
                <default-field-of-view-deg type="double">75.0</default-field-of-view-deg>
                <default-pitch-deg type="double">0</default-pitch-deg>
                <default-heading-deg type="double">270</default-heading-deg>
                <front-direction-deg type="double">0</front-direction-deg>
                <front-left-direction-deg type="double">45</front-left-direction-deg>
                <left-direction-deg type="double">90</left-direction-deg>
                <back-left-direction-deg type="double">135</back-left-direction-deg>
                <back-direction-deg type="double">180</back-direction-deg>
                <back-right-direction-deg type="double">225</back-right-direction-deg>
                <right-direction-deg type="double">270</right-direction-deg>
                <front-right-direction-deg type="double">315</front-right-direction-deg>
                <x-offset-m archive="y">-1.0</x-offset-m>
                <y-offset-m archive="y">0.60</y-offset-m>
                <z-offset-m archive="y">4.9</z-offset-m>
                <pitch-offset-deg>0</pitch-offset-deg>
            </config>
        </view>

        <systems>
            <autopilot>
                <path>Aircraft/PBY-Catalina/Systems/autopilot.xml</path>
            </autopilot>
            <electrical>
                <!-- null electrical system -->
                <path></path>
            </electrical>
        </systems>

        <instrumentation>
            <path>Aircraft/PBY-Catalina/Systems/instrumentation.xml</path>
        </instrumentation>


        <hud>
            <enable3d>false</enable3d>
        </hud>
        <multiplay>
            <generic>
                <float n="0" alias="/surface-positions/gear-pos-norm"/>
                <float n="1" alias="/gear/gear[0]/steering-norm"/>
                <float n="2" alias="/surface-positions/canopy-pos-norm"/>
                <float n="3" alias="/engines/engine[0]/thruster/rpm"/>
                <float n="4" alias="/engines/engine[1]/thruster/rpm"/>
                <float n="5" alias="/fdm/jsbsim/fcs/propeller0-rotation"/>
                <float n="6" alias="/fdm/jsbsim/fcs/propeller1-rotation"/>
                <float n="7" alias="/surface-positions/floats-pos-norm"/>
                <string n="0"></string>
            </generic>
        </multiplay>
        <mooring type="bool">true</mooring>

        <menubar include="Inputs/Catalina-menu.xml"/>

    </sim>

    <ai>
        <submodels>
            <trigger type="bool">false</trigger>
        </submodels>
    </ai>

    <controls>
        <switches>
            <landing-light>true</landing-light>
            <master-avionics>false</master-avionics>
        </switches>
        <lighting>
            <instruments-norm type="double">0</instruments-norm>
        </lighting>
        <engines>
            <engine n="0">
                <magnetos>0</magnetos>
                <master-bat>false</master-bat>
            </engine>
            <engine n="1">
                <magnetos>0</magnetos>
                <master-bat>false</master-bat>
            </engine>
        </engines>
        <electric>
            <master-switch>false</master-switch>
            <battery-switch>false</battery-switch>
            <external-power>false</external-power>
        </electric>
        <mooring>
            <automatic type="bool">true</automatic>
        </mooring>
        <gear>
            <brake-parking type="double">1.0</brake-parking>
            <gear-down>true</gear-down>
            <float-down>false</float-down>
        </gear>
    </controls>
    <engines>
        <engine n="0">
            <mp-inhg>0</mp-inhg>
            <rpm>0</rpm>
        </engine>
        <engine n="1">
            <mp-inhg>0</mp-inhg>
            <rpm>0</rpm>
        </engine>
    </engines>

    <consumables>
        <fuel>
            <tank n="1">
                <level-gal_us>400</level-gal_us>
            </tank>
            <tank n="2">
                <level-gal_us>400</level-gal_us>
            </tank>
            <tank n="4">
                <level-gal_us>350</level-gal_us>
            </tank>
            <tank n="5">
                <level-gal_us>350</level-gal_us>
            </tank>
        </fuel>
    </consumables>

    <systems>
        <mooring>
            <boat-id></boat-id>
            <dialog></dialog>
            <moorage-id></moorage-id>
            <route include="Mooring/mooring-pos.xml"/>
            <state></state>
        </mooring>
    </systems>


    <input include="Inputs/Catalina-input.xml"/>

    <nasal>
        <mooring>
            <file>Aircraft/PBY-Catalina/Mooring/PBY.nas</file>
        </mooring>
        <terrain>
            <file>Aircraft/PBY-Catalina/Systems/terrain.nas</file>
        </terrain>
    </nasal>


</PropertyList>




