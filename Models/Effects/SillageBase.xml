<?xml version="1.0"?>

<PropertyList>
<!--
    Usable with FlightGear
    Property of Gerard Robin
    Copyright License:  GPL
-->
<path>Sillage2.ac</path>

    <offsets>
            <x-m></x-m>
            <y-m>0</y-m>
            <z-m>-0</z-m>
            <pitch-deg>0.1</pitch-deg>
            <heading-deg>-0</heading-deg>
            <roll-deg>0</roll-deg>
    </offsets>





    <animation>
        <type>rotate</type>
        <object-name>flux</object-name>
        <property>sim/time/elapsed-sec</property>
        <factor>0.5</factor>
        <axis>
            <x>1</x>
       </axis>
    </animation>





</PropertyList>
