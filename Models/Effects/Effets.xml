    <animation>
        <type>select</type>
        <object-name>GFlux</object-name>
        <object-name>DFlux</object-name>
        <condition>
            <and>
                <less-than>
                    <property>/position/altitude-agl-ft</property>
                    <value>7.8</value>
                </less-than>
                <greater-than>
                    <property>/orientation/pitch-deg</property>
                    <value>-4.5</value>
                </greater-than>
            </and>
        </condition>
    </animation>

     <animation>
        <type>select</type>
        <object-name>Effet</object-name>
        <condition>
            <and>
                <less-than>
                    <property>/position/altitude-agl-ft</property>
                    <value>7.8</value>
                </less-than>
                <greater-than>
                    <property>/orientation/pitch-deg</property>
                    <value>-4.5</value>
                </greater-than>
            </and>
        </condition>
    </animation>



    <animation>
        <type>select</type>
        <object-name>GFlux1</object-name>
        <condition>
            <and>
                <equals>
                    <property>gear/gear[8]/wow</property>
                    <value type="bool">true</value>
                 </equals>
                <equals>
                    <property>surface-positions/floats-pos-norm</property>
                    <value>1.0</value>
                </equals>
            </and>
        </condition>
    </animation>



    <animation>
        <type>select</type>
        <object-name>DFlux1</object-name>
        <condition>
            <and>
                <equals>
                    <property>gear/gear[7]/wow</property>
                     <value type="bool">true</value>
                 </equals>
                <equals>
                    <property>surface-positions/floats-pos-norm</property>
                    <value>1.0</value>
                </equals>
            </and>
        </condition>
    </animation>





    <animation>
        <type>translate</type>
        <object-name>Effet</object-name>
        <property>velocities/groundspeed-kt</property>
        <interpolation>
        <entry>
            <ind>0</ind>
